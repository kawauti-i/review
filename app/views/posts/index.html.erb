<div class="row">
  <div class="search">
    <%= search_form_for @search do |f| %>
      <div class="form-group">
        <%= f.label :title_cont, "検索" %>
        <%= f.text_field :title_cont, class: "form-control" %>
      </div>
      <div class="actions"><%= f.submit "Search" %></div>
    <% end %>
  </div>
</div>
<%= render 'posts/posts', posts: @posts %>